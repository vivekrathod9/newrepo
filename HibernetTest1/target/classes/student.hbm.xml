<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.clc.hbm.StudentEntity" table="stuednthbm">
		<id name="stdId" type="int" column="studId"></id>
		<property name="stdName" type="string" column="studName"></property>
		<property name="stdAge" type="int" column="studAge"></property>
	</class>
</hibernate-mapping>